{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "AllowedOrigins": [
    "https://realserv.com",
    "https://www.realserv.com",
    "https://vendor.realserv.com",
    "https://admin.realserv.com"
  ],
  "ConnectionStrings": {
    "VendorServiceDb": "${DB_CONNECTION_STRING}",
    "Redis": "${REDIS_CONNECTION_STRING}"
  },
  "Firebase": {
    "ProjectId": "${FIREBASE_PROJECT_ID}"
  },
  "RateLimiting": {
    "PermitLimit": 200,
    "Window": 60,
    "QueueLimit": 0
  },
  "ServiceUrls": {
    "IdentityService": "${IDENTITY_SERVICE_URL}",
    "CatalogService": "${CATALOG_SERVICE_URL}",
    "OrderService": "${ORDER_SERVICE_URL}"
  }
}
